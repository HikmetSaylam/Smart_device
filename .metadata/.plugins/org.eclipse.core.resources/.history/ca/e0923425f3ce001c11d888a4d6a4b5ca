package pckg;

public class AgArayuzu 
{
	private IEyleyici eyleyici;
	private SicaklikAlgýlayýcý sicaklikAlgýlayýcý;
	
	public AgArayuzu() 
	{
		this.eyleyici=new Eyleyici();
		SicaklikAlgýlayýcýFactory sicaklikAlgýlayýcýFactory= new SicaklikAlgýlayýcýFactory();
		this.sicaklikAlgýlayýcý=sicaklikAlgýlayýcýFactory.getSicaklikAlgýlayýcý();
	}
	
	
}
